<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_startmenu"
        android:name="com.example.coffffffeeeeeeeeee.ui.startmenu.StartmenuFragment"
        android:label="@string/title_startmenu"
        tools:layout="@layout/fragment_startmenu" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.coffffffeeeeeeeeee.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.coffffffeeeeeeeeee.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.coffffffeeeeeeeeee.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_like"
        android:name="com.example.coffffffeeeeeeeeee.ui.like.Like.LikeFragment"
        android:label="@string/title_like"
        tools:layout="@layout/fragment_like" />

</navigation>